generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

enum Category {
  fragrances
  accessories
  makeup
}

model Products {
  id       Int      @id @default(autoincrement())
  img      String
  slug     String?  @unique
  name     String
  details  String
  price    Int
  category Category
}

model Banners {
  id  Int    @id @default(autoincrement())
  url String
}
